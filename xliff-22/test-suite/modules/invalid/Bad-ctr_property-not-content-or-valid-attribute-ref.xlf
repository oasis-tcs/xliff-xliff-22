<file id="crt01">
      <unit id="property-attribute-testing">
         <!-- 
            5.6.5.6 property "The value MUST be either content to signify the content of an element, 
            or the name of the attribute relating to the revision data."
         -->
         <ctr:changeTrack>
            <ctr:revisions appliesTo="note" ref="nn11">
               <ctr:revision>
                  <ctr:item property="content">old note</ctr:item><!-- good, I'll just assume the content of 
                                                                       the previous note[@id='nn1'] was "old note" -->
               </ctr:revision>
            </ctr:revisions>
         </ctr:changeTrack>
         <ctr:changeTrack>
            <ctr:revisions appliesTo="note" ref="nn22">
               <ctr:revision>
                  <ctr:item property="priority">old note</ctr:item><!--
5.6.5.6 property
MUST be either content to signify the content of an element, or the name of the attribute relating to the revision data.
--><!-- bad, had it said id or category, 
                                                                        it would have met the requirement, 
                                                                        but since note[@id='nn1'] does not have
                                                                        @priority it is a violation of 
                                                                        "or the name of the attribute relating to the revision data" -->
               </ctr:revision>
            </ctr:revisions>
         </ctr:changeTrack>         
         <notes>
            <note category="riddles" id="nn11"></note>
            <note id="nn22"></note>
         </notes>
         <segment>
            <source>Yes</source>
            <target>Ja</target>
         </segment>
      </unit>
   </file>
